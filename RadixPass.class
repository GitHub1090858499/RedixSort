public class Main
{
    
    //一趟基数排序
    public static void oneRadixPass(int[] a,int[] a2,int[] count,int[] pos,int radix){
        //位置、个数的数组置零
        for(int i=0;i<pos.length;i++){
            pos[i]=0;
            count[i]=0;
        }
        
        //遍历a的radix位,计数同数个数于count
        for(int i=0;i<a.length;i++){
            count[a[i]/radix%10]++; 
        }
        
        //遍历count，生成pos位置数组
        for(int i=1;i<count.length;i++){
            pos[i]=pos[i-1]+count[i-1];
        }
        
        //按pos位置信息的a的radix位摆放a到a2
        for(int i=0;i<a.length;i++){
            a2[pos[a[i]/radix%10]]=a[i];
            pos[a[i]/radix%10]++;
        }
        
        
    }
    //按weight位，r进制，weight趟排序
    public static int[] radixPass(int[] a,int weight,int r){
        int[] b1=a;
        int[] b2;
        int[] b3=new int[a.length];
	    //进位为r
	    int[] count=new int[r];
	    int[] pos=new int[r];
	    
	    int radix=1;
	    for(int i=0;i<weight;i++){
    	    oneRadixPass(b1,b3,count,pos,radix);
    	    b2=b3;
    	    b3=b1;
    	    b1=b2;
    	    radix=radix*r;
	    }
	    return b1;
    }
    

	public static void main(String[] args) {
	    int[] a={337,332,132,267,262,164,260,167};	   
	    a=radixPass(a,3,10);
	    for(int i=0;i<a.length;i++){
	    System.out.println(a[i]);
	    }
		
	}
}
